{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true
  },

  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    },
    "Microsoft.Extensions.Configuration.Json": "1.1.0",
    "Microsoft.Extensions.Logging.Debug": "1.1.0",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.1.0",

    "<%= appname %>.Core": "*",

    "Prospero.Application.Console.Conventions": "1.0.0-*",
    "Prospero.DataAccess.EFCore.Conventions": "1.0.0-*",
    "Prospero.Extensions.EntityFramework.Conventions.SqlServer": "1.0.0-*",
    "Prospero.Extensions.AutoMapper.Conventions": "1.0.0-*",
    "Prospero.Extensions.Logging.Conventions": "1.0.0-*"
  },
  "publishOptions": {
    "include": [
	  "run.cmd",
      "appsettings.json"
    ]
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50"
    }
  }
}
